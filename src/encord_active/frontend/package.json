    "@formkit/auto-animate": "^1.0.0-beta.6",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tanstack/react-query": "^4.29.1",
    "@tanstack/react-query-devtools": "^4.29.19",
    "antd": "^5.8.6",
    "axios": "^1.4.0",
    "d3-array": "^3.2.3",
    "d3-hexbin": "^0.2.2",
    "d3-scale": "^4.0.2",
    "daisyui": "^2.51.3",
    "radash": "^10.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router": "^6.14.0",
