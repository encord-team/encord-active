    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "hotscript": "^1.0.12",
    "postcss": "^8.4.18",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "tailwindcss": "^3.1.8",
    "typescript": "^5.1.6",
    "vite": "^4.1.5"
