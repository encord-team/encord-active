    "tsc": "tsc",
    "lint:es": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:es:fix": "eslint src --fix --ext .js,.jsx,.ts,.tsx",
    "prettier:dry": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "preview": "vite preview"
  },
  "dependencies": {
