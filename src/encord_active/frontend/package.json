    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/recharts": "^1.8.24",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.12",
    "concurrently": "^8.2.0",
